{
  "expo": {
    "name": "Almuslim",
    "slug": "almuslim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Almuslim-logo.png",
    "scheme": "almuslim",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#006a4e"
    },
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.almuslim.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#006a4e"
      },
      "package": "com.almuslim.app",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/Almuslim-logo.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Almuslim to access your location for accurate prayer times."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#006a4e"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "15292faf-f819-4f84-839b-b3d1bf580350"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/15292faf-f819-4f84-839b-b3d1bf580350"
    }
  }
}
